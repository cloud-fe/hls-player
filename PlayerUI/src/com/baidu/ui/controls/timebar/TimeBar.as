package com.baidu.ui.controls.timebar{		import flash.display.MovieClip;	import flash.text.TextField;			public class TimeBar extends MovieClip {				private var timeTxt:TextField = mcTimeTxt;				private var totalTxt:TextField = mcTotalTxt;				public function TimeBar() {			timeTxt=mcTimeTxt;			totalTxt=mcTotalTxt;		}		public function set time(s:String):void{			timeTxt.text = s;		}		public function set total(s:String):void{			totalTxt.text = s;		}	}	}